{
  "name" : "my-application/application-base",
  "description" : "My Web-Application (http://my-application.net)",
  "version" : "1.0.0.1",
  "require" : {
    "php" : ">=5.3.3",
    "zendframework/zendframework" : "dev-master",
    "zf-commons/zfc-user" : ">=1.2.2",
    "neilime/zf2-assets-bundle" : ">=3.4.1",
    "zendexperts/ze-theme" : "dev-master",
    "dudapiotr/zftable" : ">=3.1",
    "org_heigl/piwik" : ">=1.0",
    "my-application/module-admin" : "dev-master",
    "my-application/module-twbs-api" : "dev-master",
    "components/jquery" : ">=1.11.3",
    "components/jqueryui" : ">=1.11.4",
    "components/foundation" : ">=5.5.2",
    "components/html5-boilerplate" : ">=4.3.0",
    "twitter/bootstrap" : ">=v3.3.5",
    "fortawesome/font-awesome" : ">=4.4.0",
    "newerton/fancy-box" : "dev-master",
    "tinymce/tinymce" : ">=4.2.1",
    "themes/zf2-basic" : "dev-master",
    "themes/zf2-adminlte" : "dev-master",
    "themes/zf2-remark" : "dev-master",
    "themes/zf2-taurus" : "dev-master"
  },
  "extra" : {
    "branch-alias" : {
      "dev-master" : "master"
    }
  },
  "repositories" : [ {
    "type" : "git",
    "url" : "http://gitlab.dragon-projects.de:81/themes/zf2-basic.git"
  }, {
    "type" : "git",
    "url" : "http://gitlab.dragon-projects.de:81/themes/zf2-adminlte.git"
  }, {
    "type" : "git",
    "url" : "http://gitlab.dragon-projects.de:81/themes/zf2-remark.git"
  }, {
    "type" : "git",
    "url" : "http://gitlab.dragon-projects.de:81/themes/zf2-taurus.git"
  }, {
    "type" : "git",
    "url" : "http://gitlab.dragon-projects.de:81/zf2/module-admin.git"
  }, {
    "type" : "git",
    "url" : "http://gitlab.dragon-projects.de:81/zf2/module-twbs-api.git"
  } ],
  "license" : "BSD-3-Clause",
  "keywords" : [ "myapplication", "application", "framework", "zf2" ],
  "type" : "project",
  "scripts" : {
    "post-install-cmd" : [ "chmod -R ugoa+rx ./shell", "./shell/files.components.sh", "./shell/files.themes.sh" ],
    "post-update-cmd" : [ "chmod -R ugoa+rx ./shell", "./shell/files.components.sh", "./shell/files.themes.sh" ],
    "never-to-be-executed" : [ "rm -rf components" ]
  },
  "homepage" : "http://dragon-projects.net/"
}